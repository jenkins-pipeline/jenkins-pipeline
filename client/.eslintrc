{
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "jasmine": true
  },
  "globals": {
    "_": true,
    "$": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-console": 0,
    "strict": [ 2, "global" ],
    "camelcase": [ 2, { "properties": "always" } ],
    "linebreak-style": 2,
    "block-scoped-var": 2,
    "brace-style": [ 2, "1tbs", { "allowSingleLine": true } ],
    "semi": [ 2, "always" ],
    "no-native-reassign": 2,
    "eqeqeq": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "radix": [ 2, "always" ],
    "no-loop-func": 2,
    "wrap-iife": [ 2, "inside" ],
    "dot-location": [ 2, "object" ],
    "dot-notation": 1,
    "no-magic-numbers": 1,
    "no-else-return": 1,
    "quotes": [ 1, "single" ]
  }
}
