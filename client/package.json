{
  "name": "kickstart-brunch",
  "version": "1.0.0",
  "description": "Personal brunch io kickstarter with javascript, SASS, eslint and jasmine",
  "scripts": {
    "lint": "esw app/ spec/ --ext .js",
    "_clean": "rm -Rv ../public/* | true",
    "test": "jasmine",
    "prebuild": "npm run lint && npm run test && npm run _clean",
    "build": "brunch build",
    "build:prod": "npm run build -- -p",
    "prestart": "npm run prebuild",
    "start": "brunch watch --server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/junior-ales/kickstart-brunch.git"
  },
  "author": "@junior_ales",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/junior-ales/kickstart-brunch.io/issues"
  },
  "devDependencies": {
    "auto-reload-brunch": "^1.8.1",
    "brunch": "^2.1.3",
    "clean-css-brunch": "^1.8.0",
    "css-brunch": "^1.7.0",
    "eslint": "^1.10.3",
    "eslint-watch": "^2.1.5",
    "jasmine": "^2.4.1",
    "javascript-brunch": "^1.8.0",
    "sass-brunch": "^1.9.2",
    "uglify-js-brunch": "^1.7.8"
  }
}
